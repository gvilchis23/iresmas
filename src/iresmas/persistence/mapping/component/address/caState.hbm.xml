<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<!--Author: Cristopher Cerrillo Callejas -->
	<!--Representa el mapeo objeto relacional entre la clase y la tabla  -->
<hibernate-mapping package="iresmas.persistence.entity.component.address">
  <class name="CaState" table="ca_states">
  	<id name="idState" column="IDSTATE">
  		<generator class="native"/>
  	</id>
  	<property name="idCountry" column="IDCOUNTRY" not-null="true" type="integer"/>
  	<property name="stateName" column="STATENAME" not-null="true" type="string"/>

  </class>
</hibernate-mapping>